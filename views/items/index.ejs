<!DOCTYPE html>
<html>
  <head>
    <title>Items</title>
  </head>
  <body>
    <h1>All Items</h1>

    <ul>
      <% items.forEach(item => { %>
      <li>
        <%= item.name %> — Qty: <%= item.quantity %>, $<%= item.price %> —
        Category: <%= item.category_name %>
      </li>
      <% }) %>
    </ul>

    <a href="/categories">← Back to Categories</a>
  </body>
</html>
